    {
      "$TYPE":"TurnPitchOutput"
    },
    {
      "$TYPE":"LeftClickOutput"
    },
    {
      "$TYPE":"RightClickOutput"
    },
    {
      "$TYPE":"WalkForwardOutput"
    },
    {
      "$TYPE":"WalkSidewaysOutput"
    },
    {
      "$TYPE":"JumpOutput"
